
pkgname=qt5-doc
pkgver=5.3.0RC
subver=5.3.0-RC
_pkgver=5.3
pkgrel=1
pkgdesc=('A cross-platform application and UI framework, offline documentation files')
arch=('x86_64')
url='http://qt.nokia.com/'
license=('GPL3' 'LGPL')
depends=('qt5-tools')
options=('!libtool')
_pkgfqn="qtdoc-opensource-src-${subver}"
source=("http://download.qt-project.org/development_releases/qt/${_pkgver}/5.3.0-rc/submodules/$_pkgfqn.tar.xz")
md5sums=('fed953d794c1afd3f224731e9d630095')

build() {
    cd $srcdir/$_pkgfqn
 
    /usr/lib/qt5/bin/qmake
    make docs
}

package() {
    cd $srcdir/$_pkgfqn
    
    make INSTALL_ROOT=$pkgdir install_docs
}

