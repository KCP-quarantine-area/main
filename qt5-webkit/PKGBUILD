
pkgname=qt5-webkit
pkgver=5.2.1
_pkgver=5.2
pkgrel=1
pkgdesc=('A cross-platform application and UI framework, QtWebkit files')
arch=('x86_64')
url='http://qt.nokia.com/'
license=('GPL3' 'LGPL' 'FDL')
depends=('qt5-sensors' 'qt5-location' 'qt5-quickcontrols' 'icu' 'libxslt' 'libxcomposite' 'gstreamer0.10'
         'gstreamer0.10-base' 'gstreamer0.10-base-plugins' 'gstreamer0.10-good-plugins' 'libwebp'
         'gstreamer0.10-bad-plugins' 'gstreamer0.10-ffmpeg' 'gstreamer0.10-ugly-plugins' 'mesa'
         'qt5-serialport' 'qt5-multimedia' 'qt5-imageformats')
makedepends=('gperf' 'sqlite' 'fontconfig' 'perl' 'python2' 'ruby')
options=('!libtool')
groups=('qt5')
_pkgfqn="qtwebkit-opensource-src-${pkgver}"
source=("http://download.qt-project.org/official_releases/qt/${_pkgver}/${pkgver}/submodules/$_pkgfqn.tar.xz")
md5sums=('a20d99a1c2afbd6a7c6f54e615f1bd63')

build() {
   cd $srcdir/$_pkgfqn
	
   /usr/lib/qt5/bin/qmake
   make
}

package() {
   cd $srcdir/$_pkgfqn
   make INSTALL_ROOT=$pkgdir install
    
   #mkdir -p ${pkgdir}/usr/bin
   #for i in $(ls ${pkgdir}/usr/lib/qt5/bin); do
   #    ln -s /usr/lib/qt5/bin/${i} ${pkgdir}/usr/bin/${i}-qt5
   #done
}

